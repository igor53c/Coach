<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="igra.ipcoding.coach">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_round"
        android:supportsRtl="true"
        android:largeHeap="true"
        android:hardwareAccelerated="false"
        android:theme="@style/Theme.Coach">
        <activity android:name="igra.ipcoding.coach.view.GlavniActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="igra.ipcoding.coach.view.BiranjeKlubaActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="igra.ipcoding.coach.view.ObjasnjenjeActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="igra.ipcoding.coach.view.PredMecActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="igra.ipcoding.coach.view.ZamenaIgracaActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="igra.ipcoding.coach.view.TransferActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="igra.ipcoding.coach.view.UvodActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>



</manifest>